{
  "build": {
    "commands": [
      "cd frontend && npm install && npm run build",
      "cd backend && pip install -r requirements.txt",
      "cd backend && python manage.py collectstatic --noinput"
    ]
  },
  "deploy": {
    "startCommand": "cd backend && python manage.py runserver 0.0.0.0:$PORT"
  }
}
